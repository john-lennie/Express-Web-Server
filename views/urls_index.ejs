<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_header %>
</head>
<body>

    <% for (var i = 0; i < urls.length; i++) { %>
      <% if (user_id === urls[i].userID) { %>

        <ul>
          <li><%= urls[i].url %>, <%= urls[i].id %></li>
        </ul>

        <a href="/urls/<%= urls[i].id %>">Edit</a>
        <form action="/urls/<%= urls[i].id %>/delete" method="POST"><input type="submit" value="Delete"></form>
        
      <% } %>
    <% } %>

</body>
</html>
